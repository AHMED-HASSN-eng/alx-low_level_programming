#include <stdio.h>
#include <unistd.h>
#include <main.h>
/**
 * short deccription
 * main - Entry point
 * Description: '_putchar - writes the character c to stdout'
 * The character to print
 *
 * Return: always return 0.
 * On error, -1 is returned, and errno is set appropriately.
 */
int main(void)
{
_putchar('_');
_putchar('p');
_putchar('u');
_putchar('t');
_putchar('c');
_putchar('h');
_putchar('a');
_putchar('r');
_putchar("\n");
return (0);
}
